{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "pantheon-systems/drupal-integrations": "^9",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11 || ^12",
        "drupal/insert_view": "^2.0",
        "drupal/markdown": "^3.0@RC",
        "league/commonmark": "^1.0",
        "erusev/parsedown-extra": "^0.8.1",
        "drupal/paranoia": "^1.0@RC",
        "drupal/admin_toolbar": "^3.1",
        "drupal/allowed_formats": "^1.5",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/captcha": "^1.5",
        "drupal/components": "^2.4",
        "drupal/content_moderation_notifications": "^3.4",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.23",
        "drupal/honeypot": "^2.1",
        "drupal/metatag": "^1.21",
        "drupal/twig_tweak": "^3.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/security_review": "^1.0@alpha",
        "drupal/scheduler_content_moderation_integration": "^1.4",
        "drupal/redirect": "^1.8",
        "drupal/recaptcha": "^3.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/pathauto": "^1.11",
        "drupal/smart_date": "^3.7@beta",
        "drupal/mediteran": "^1.27",
        "drupal/paragraphs": "^1.15",
        "drupal/password_policy": "^3.2",
        "drupal/field_group": "^3.3",
        "drupal/address": "^1.11",
        "drupal/office_hours": "^1.7",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/migrate_source_csv": "3.x-dev",
        "drupal/datalayer": "^1.0@beta",
        "drupal/semanticviews": "^2.3",
        "drupal/tb_megamenu": "^2.0@alpha",
        "drupal/site_alert": "^1.2",
        "symfony/http-client": "^5.4",
        "geocoder-php/google-maps-provider": "^4.7",
        "drupal/geofield": "^1.46",
        "drupal/geofield_map": "^2.88",
        "drupal/geocoder_address": "^3",
        "drupal/config_ignore": "^2.3",
        "drupal/feeds": "^3.0@beta",
        "drupal/search_api": "^1.28",
        "drupal/search_api_solr": "^4.2",
        "drupal/search_api_attachments": "^9.0",
        "drupal/term_reference_fancytree": "^2.0@RC",
        "drupal/hook_event_dispatcher": "^3.3",
        "drupal/high_contrast": "^1.0@beta",
        "drupal/remote_stream_wrapper": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/registration": "^3.0@beta",
        "drupal/queue_ui": "^3.1",
        "drupal/extlink": "^1.7",
        "drupal/customfilter": "^2.0",
        "drupal/insert_block": "^1.0@beta",
        "drupal/insert_view_adv": "^2.0@alpha",
        "drupal/entity_clone": "^2.0@alpha",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/masquerade": "^2.0@RC",
        "drupal/webform": "^6.1",
        "drupal/webform_migrate": "^1.2",
        "drupal/field_permissions": "^1.2",
        "drupal/hierarchical_term_formatter": "^1.2",
        "drupal/permissions_filter": "^1.3",
        "drupal/embed_block": "^1.0@alpha"
    },
    "require-dev": {
        "drupal/core-dev": "^9.2",
        "squizlabs/php_codesniffer": "^3.6",
        "drupal/coder": "^8.3",
        "drupal/drupalmoduleupgrader": "^1.9",
        "jcnventura/pharborist": "dev-master",
        "drupal/devel": "^5.1",
        "phpspec/prophecy-phpunit": "^2",
        "drupal/webprofiler": "^9.0",
        "drupal/examples": "^3.0"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/mediteran": {
                "Width for Hour select field": "https://www.drupal.org/files/issues/2022-11-17/mediteran-hourselectfieldwidth-3322239-2.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "upstream-configuration/scripts/ComposerScripts.php"
        ]
    },
    "scripts": {
        "pre-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::preUpdate"
        ],
        "upstream-require": [
            "DrupalComposerManaged\\ComposerScripts::upstreamRequire"
        ]
    },
    "scripts-descriptions": {
        "upstream-require": "Add a dependency to an upstream. See https://pantheon.io/docs/create-custom-upstream for information on creating custom upstreams."
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    }
}
