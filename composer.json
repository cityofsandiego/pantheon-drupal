{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "type": "drupal-library",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/jackmoore/colorbox.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "cure53/dompurify",
                "version": "3.0.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/cure53/dompurify/archive/3.0.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "woocommerce/flexslider",
                "version": "2.7.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/woocommerce/FlexSlider.git",
                    "type": "git",
                    "reference": "2.7.2"
                }
            }
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "pantheon-systems/drupal-integrations": "^9",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11 || ^12",
        "drupal/insert_view": "^2.0",
        "drupal/markdown": "^3.0@RC",
        "league/commonmark": "^1.0",
        "erusev/parsedown-extra": "^0.8.1",
        "drupal/paranoia": "^1.0@RC",
        "drupal/admin_toolbar": "^3.1",
        "drupal/allowed_formats": "^2.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/captcha": "^1.5",
        "drupal/components": "^2.4",
        "drupal/content_moderation_notifications": "^3.4",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/focal_point": "^2.0",
        "drupal/fontawesome": "^2.23",
        "drupal/honeypot": "^2.1",
        "drupal/metatag": "^1.21",
        "drupal/twig_tweak": "^3.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/scheduler_content_moderation_integration": "^1.4",
        "drupal/redirect": "^1.8",
        "drupal/recaptcha": "^3.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/pathauto": "^1.11",
        "drupal/smart_date": "^3.7@beta",
        "drupal/mediteran": "^1.27",
        "drupal/paragraphs": "^1.15",
        "drupal/password_policy": "^3.2",
        "drupal/field_group": "^3.3",
        "drupal/address": "^1.11",
        "drupal/office_hours": "^1.7",
        "drupal/datalayer": "^1.0@beta",
        "drupal/semanticviews": "^2.3",
        "drupal/tb_megamenu": "^1.7",
        "symfony/http-client": "^5.4",
        "geocoder-php/google-maps-provider": "^4.7",
        "drupal/geofield": "^1.46",
        "drupal/geofield_map": "^2.88",
        "drupal/geocoder_address": "^3",
        "drupal/feeds": "^3.0@beta",
        "drupal/search_api": "1.29",
        "drupal/search_api_solr": "^4.2",
        "drupal/search_api_attachments": "^9.0",
        "drupal/term_reference_fancytree": "^2.0@RC",
        "drupal/hook_event_dispatcher": "^3.3",
        "drupal/high_contrast": "^1.0@beta",
        "drupal/remote_stream_wrapper": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/registration": "^3.0@beta",
        "drupal/queue_ui": "^3.1",
        "drupal/extlink": "^1.7",
        "drupal/customfilter": "^2.0",
        "drupal/insert_block": "^1.0@beta",
        "drupal/insert_view_adv": "^2.0@alpha",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/masquerade": "^2.0@RC",
        "drupal/webform": "^6.1",
        "drupal/field_permissions": "^1.2",
        "drupal/hierarchical_term_formatter": "^1.2",
        "drupal/tamper": "^1.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/asset_injector": "^2.16",
        "drupal/views_data_export": "^1.3",
        "drupal/draggableviews": "^2.1",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/colorbox": "^2.0",
        "drupal/colorbox_inline": "^2.0",
        "drupal/colorbox_load": "^1.3",
        "jackmoore/colorbox": "dev-master",
        "drupal/html_title": "^1.4",
        "drupal/cacheexclude": "^2.0",
        "drupal/views_conditional": "^1.5",
        "drupal/facets": "^3.0@beta",
        "drupal/search_api_exclude_entity": "^2.0",
        "drupal/views_selective_filters": "1.x-dev@dev",
        "drupal/views_taxonomy_term_name_into_id": "^1.0@RC",
        "drupal/quick_node_clone": "^1.16",
        "drupal/calendar": "^1.0@alpha",
        "drupal/calendar_datetime": "^1.0@alpha",
        "drupal/views_exposed_filter_blocks": "^1.2",
        "drupal/views_entity_embed": "^2.0",
        "drupal/views_taxonomy_term_name_depth": "^7.2",
        "drupal/views_dependent_filters": "^1.0",
        "drupal/contextual_range_filter": "^2.0",
        "drupal/simplesamlphp_auth": "^3.3",
        "drupal/views_bulk_edit": "^2.8",
        "drupal/pantheon_advanced_page_cache": "^2.1",
        "drupal/calendar_view": "^2.1",
        "drupal/flexslider": "^2.0",
        "woocommerce/flexslider": "~2.0",
        "drupal/sitewide_alert": "^2.0",
        "drupal/smtp": "^1.2",
        "drupal/base_field_override_ui": "^1.1",
        "drupal/views_year_filter": "^2.1",
        "drupal/cancel_button": "^1.4",
        "drupal/term_reference_tree": "^2.0",
        "drupal/views_alias_filter": "^1.0",
        "drupal/workbench": "^1.4",
        "drupal/override_node_options": "^2.7",
        "cure53/dompurify": "^3.0",
        "drupal/reroute_email": "^2.2",
        "drupal/seckit": "^2.0",
        "drupal/permissionspolicy": "^1.1",
        "simplesamlphp/simplesamlphp": "^1.19",
        "drupal/redis": "^1.7",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/anchor_link": "^2.6",
        "drupal/fakeobjects": "^1.2",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/userprotect": "^1.2",
        "drupal/linkit": "^6.0",
        "drupal/linkit_media_library": "^1.0",
        "drupal/media_entity_file_replace": "^1.1",
        "drupal/media_file_delete": "^1.3",
        "drupal/genpass": "^2.0",
        "drupal/permissions_filter": "^1.5",
        "drupal/filter_perms": "^2.0@RC",
        "drupal/ckeditor_liststyle": "^1.5",
        "drupal/protected_pages": "^1.6",
        "drupal/core-composer-scaffold": "~9.5.11",
        "drupal/core-dev": "~9.5.11",
        "drupal/core-recommended": "~9.5.11",
        "drupal/core": "~9.5.11",
        "drupal/config_ignore": "^3.1",
        "drupal/config_filter": "^2.6",
        "drupal/dropzonejs": "^2.8",
        "drupal/simple_media_bulk_upload": "^1.0",
        "drupal/restui": "^1.21",
        "drupal/config_partial_export": "^2.0@alpha",
        "drupal/environment_indicator": "^4.0",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/ckeditor5_fullscreen": "^1.0@beta",
        "drupal/upgrade_status": "^4.1",
        "drupal/selective_better_exposed_filters": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "drupal/coder": "^8.3",
        "jcnventura/pharborist": "dev-master",
        "drupal/devel": "^5.1",
        "phpspec/prophecy-phpunit": "^2",
        "drupal/examples": "^3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"],
            "libraries/{$name}": ["type:drupal-library"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Add date granularity option back to contextual views filters.": "https://www.drupal.org/files/issues/2023-02-27/2868014-134.patch",
                "Undefined offset": "https://www.drupal.org/files/issues/2022-03-19/3196247-13_0.patch",
                "Hack for calendar_view and smart date": "patches/core_hack1.patch",
                "Admin menu sort Alphabetical": "https://www.drupal.org/files/issues/2020-11-26/sort_menu_list-3070721-9.patch",
                "Allow iframe in view rewrite and global text.": "patches/iframe_for_views_rewrite_global_text_idea_from_3019171.patch"
            },
            "drupal/calendar_view": {
                "Hack for calendar_view and smart date": "patches/calendar_view_hack1.patch",
                "Hack remove timezone adjustment": "patches/calendar_view_hack2.patch"
            },
            "drupal/colorbox": {
                "Allow media entity reference fields to be displayed as a gallery in a colorbox": "https://www.drupal.org/files/issues/2023-03-31/2957030-42-colorbox-entity_reference_formatter.patch"
            },
            "drupal/draggableviews": {
                "Fix RouteNotFound": "https://www.drupal.org/files/issues/2023-03-16/draggableviews-route-error-3316975-15.patch"
            },
            "drupal/insert_block": {
                "Fix block issue with custom block insertions.": "patches/insert_custom_block.patch"
            },
            "drupal/views_selective_filters": {
                "Pending bugfix for issue #2804485.": "https://www.drupal.org/files/issues/2020-07-13/views_selective_filters-sql_error-2804485-9-8.patch"
            },
            "drupal/ultimate_cron": {
                "Pending bugfix for issue #3001044.": "https://www.drupal.org/files/issues/2018-09-20/ultimate_cron-3001044-2.patch"
            }
        },
        "enable-patching": true
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
"scripts": {
    "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
    "post-update-cmd": [
        "DrupalComposerManaged\\ComposerScripts::postUpdate",
        "rm -rf vendor/simplesamlphp/simplesamlphp/config && ln -sf ../../../private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
        "rm -rf vendor/simplesamlphp/simplesamlphp/metadata && ln -sf ../../../private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata",
        "rm -rf vendor/simplesamlphp/simplesamlphp/cert && ln -sf ../../../private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert"
    ],
    "post-install-cmd": [
        "rm -rf vendor/simplesamlphp/simplesamlphp/config && ln -sf ../../../private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
        "rm -rf vendor/simplesamlphp/simplesamlphp/metadata && ln -sf ../../../private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata",
        "rm -rf vendor/simplesamlphp/simplesamlphp/cert && ln -sf ../../../private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert"
    ]
},
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "simplesamlphp/composer-module-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
